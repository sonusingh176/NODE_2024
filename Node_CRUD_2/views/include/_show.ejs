<% for(var i=0; i< users.length;i++){%>
<tr>

    <th scope="row"><%=  i+1%></th>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <th><i class="bi bi-check-circle-fill"></i><%= users[i].status %></th>
    <th><a href="" class="btn btn-danger" >Delete</button></th>
    <th><a href="/update-user?id=<%= users[i]._id %>" class="btn btn-success">Update</a></th>
</tr>
<%}%>